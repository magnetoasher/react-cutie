    "icon",
    "ui",
    "animation",
    "icons",
    "cute",
    "mbe",
    "cartoons",
    "design"
  ],
  "author": "Miukimiu",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/miukimiu/react-kawaii/issues"
  },
  "homepage": "https://github.com/miukimiu/react-kawaii#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
