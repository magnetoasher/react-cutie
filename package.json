    "styleguide:build": "styleguidist build",
    "build": "webpack -p --progress",
    "prepublish": "npm run build",
    "deploy": "gh-pages -d styleguide",
    "lint": "eslint src/**/*.jsx",
    "lint:fix": "eslint --fix src/**/*.jsx"
